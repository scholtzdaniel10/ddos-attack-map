{
  "name": "ddos-attack-map",
  "version": "1.0.0",
  "description": "3D DDoS Attack Visualization Map with ML-based threat classification",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:all": "npm install && cd client && npm install && npm run build",
    "ml-train": "python ml/train_model.py",
    "install-all": "npm install && cd client && npm install",
    "vercel-build": "cd client && npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "geoip-lite": "^1.4.7",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ddos",
    "visualization",
    "3d",
    "globe",
    "cybersecurity",
    "threejs",
    "machine-learning"
  ],
  "author": "Your Name",
  "license": "MIT"
}
